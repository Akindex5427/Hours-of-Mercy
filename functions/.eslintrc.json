{
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "quotes": ["error", "double"],
    "import/no-unresolved": 0,
    "indent": ["error", 2],
    "max-len": ["error", { "code": 120 }]
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  }
}
